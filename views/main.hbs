 {{!-- main --}}
 <h2>Productos</h2>
{{#if productos }}
<button class="btn btn-success" onclick= "window.location.href = '/'">agregar nuevo producto</button>
 <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">producto</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen de producto</th>
    </tr>
  </thead>
  <tbody>
       {{#each productos}}
    <tr>
      <th scope="row">{{ this.id }}</th>
      <td>{{ this.title }}</td>
      <td>${{ this.price }}</td>
      <td><img src="{{this.thumbnail}}" width="100" alt=""></td>
    </tr>
     {{/each}}
  </tbody>
</table>

{{else}}
    <div class="alert alert-danger" role="alert">
    {{error}}
    </div>
{{/if }}

